{
  "name": "bitwasp/bitcoind-server",
  "description": "A helper library for spawning bitcoind regtest nodes for unit tests",
  "keywords": ["unit test", "bitcoin", "daemon", "regtest"],
  "homepage": "https://github.com/bit-wasp/bitcoind-server",
  "license": "MIT",
  "require": {
    "bitwasp/buffertools": "^0.5.0",
    "nbobtc/bitcoind-php": "v2.0.2",
    "matomo/ini": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "BitWasp\\Bitcoind\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitWasp\\Test\\Bitcoind\\": "test/unit/"
    }
  }
}
